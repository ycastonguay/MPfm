<Application x:Class="Sessions.WPF.Classes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="App_Startup">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Styles/Colors.xaml" />
        <ResourceDictionary Source="Styles/Buttons.xaml" />
        <ResourceDictionary Source="Styles/HeaderButtons.xaml" />
        <ResourceDictionary Source="Styles/TabButtons.xaml" />
        <ResourceDictionary Source="Styles/ComboBoxes.xaml" />
        <ResourceDictionary Source="Styles/Menus.xaml" />
        <ResourceDictionary Source="Styles/ListViews.xaml" />
        <ResourceDictionary Source="Styles/Hyperlinks.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>   
</Application>
