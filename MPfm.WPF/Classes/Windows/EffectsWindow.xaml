                <Label Grid.Row="2" x:Name="lblValue1" Content="0.0 dB" Foreground="White" FontSize="11" FontFamily="/Resources/Fonts/#Roboto Regular" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Padding="2" />
